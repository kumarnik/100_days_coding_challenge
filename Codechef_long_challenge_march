4) Space Array:


#include<bits/stdc++.h>
#include <iostream>


using namespace std;

int main()
{
    int t,n;
    cin>>t;
    while(t>0)
    {
        cin>>n;
        int a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        sort(a,a+n);
        int sum=0,flag=0;
        for(int i=0;i<n;i++)
        {
            if(a[i]>(i+1))
            {
                flag=1;
                break;
            }
            else
            {
                sum=sum+abs(a[i]-(i+1));
            }
        }
        if((flag==1) ||((sum%2)==0))
        {
            cout<<"Second"<<endl;
        }
        else
        {
            cout<<"First"<<endl;
            
        }
        t--;
        
    }

    return 0;
}



