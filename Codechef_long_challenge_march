#include<bits/stdc++.h>
#include <iostream>


using namespace std;

int main()
{
   int t,N,E,H,A,B,C;
   cin>>t;
   int min,sum1,min1,min2,min3,max,count,a,b,c,left_e,left_h,count_hh,count_eggs;
   while(t>0)
   {
       sum1=0,count=0,a=0,b=0,c=0;
       cin>>N>>E>>H>>A>>B>>C;
       left_h=H;
       left_e=E;
       count_eggs=0;
       count_hh=0;
       sum1=A+B+C;
       min1=(A<B?A:B)<C?(A<B?A:B):C;
       min3=(A>B?A:B)>C?(A>B?A:B):C;
       min2=sum1-(min1+min3);
       cout<<min1<<min2<<min3<<endl;
       max=E>H?E:H;
       min=(E+H)-max;
       cout<<min<<endl;
       if(N>=max)
       {
           cout<<-1<<endl;
       }
       else
       {
           while(count<N)
           {
               if(min1==C)
               {
                   if(min>=N)
                   {
                       a=0;
                       b=0;
                       c=N;
                       break;
                   }
                   else
                   {
                       count=count+min;
                       left_e=E-min;
                       left_h=H-min;
                       c=c+min;
                       
                   }
               }
               
               
               if(min1==B)
               {
                   count_hh=H/3;
                   left_h=H-count_hh;
                   b=b+count_hh;
                   count=count+b;
                   
                   
               }
               
               if(min1==A)
               {
                   count_eggs=E/2;
                   left_e=E-count_eggs;
                   a=a+count_eggs;
                   count=count+a;
                   
               }
           }
           
           
           
           
           
       }
       cout<<(A*a+B*b+C*c)<<endl;
       
       
       t--;
   }

}
