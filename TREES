TRAVERSALS:
PREORDER

/* A binary tree node has data, pointer to left child
   and a pointer to right child  

/*
struct Node
{
    int data;
    struct Node* left;
    struct Node* right;
    
    Node(int x){
        data = x;
        left = right = NULL;
    }
};
*/
void preutil(Node* root,vector<int>&v)
{
    if(!root)
  {
      return;
  }
  
  v.push_back(root->data);
  preutil(root->left,v);
  preutil(root->right,v);
    
}

vector <int> preorder(Node* root)
{
  // Your code here
  vector<int> s;
  preutil(root,s);
  return s;
}
